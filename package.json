{
  "name": "pathify",
  "displayName": "pathify",
  "description": "Resolve and open file from path string",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "pathify.openFileOnPath",
      "title": "Open File On Path"
    }],
    "keybindings": [
      {
          "command": "pathify.openFileOnPath",
          "key": "alt+d",
          "mac": "alt+d",
          "when": "!terminalFocus"
      }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
